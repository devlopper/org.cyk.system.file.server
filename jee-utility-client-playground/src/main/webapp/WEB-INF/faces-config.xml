<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		
		<navigation-case>
			<from-outcome>openedfromcommandbutton</from-outcome>
			<to-view-id>/component/command/commandbutton/openedfromcommandbutton.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>namableEditView</from-outcome>
			<to-view-id>/private/namable/edit.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>namableListView</from-outcome>
			<to-view-id>/private/namable/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>personTypeEditView</from-outcome>
			<to-view-id>/private/persontype/edit.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>personTypeListView</from-outcome>
			<to-view-id>/private/persontype/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>personTypeSelectManyView</from-outcome>
			<to-view-id>/private/persontype/selectmany.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>personTypeSelectOneView</from-outcome>
			<to-view-id>/private/persontype/selectone.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>personEditView</from-outcome>
			<to-view-id>/private/person/edit.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>personListView</from-outcome>
			<to-view-id>/private/person/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>google</from-outcome>
			<to-view-id>http://www.google.com</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>outcome</from-outcome>
			<to-view-id>/outcome.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
	</navigation-rule>
	   
</faces-config>
