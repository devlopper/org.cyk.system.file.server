<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		
		<navigation-case>
			<from-outcome>pageIndexView</from-outcome>
			<to-view-id>/page/index.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>pageCustomView</from-outcome>
			<to-view-id>/page/custom.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>pageNoThemeView</from-outcome>
			<to-view-id>/page/notheme.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>inputIndexView</from-outcome>
			<to-view-id>/input/index.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>inputTextView</from-outcome>
			<to-view-id>/input/text.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>inputFileView</from-outcome>
			<to-view-id>/input/file.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>navigation01</from-outcome>
			<to-view-id>/p1/p2/p3.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>navigation02</from-outcome>
			<to-view-id>/p1/p2/p3/p4/p5.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>navigationIndexView</from-outcome>
			<to-view-id>/navigation/index.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>commandableButtonIndexView</from-outcome>
			<to-view-id>/commandable/button/index.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>commandableNavigationOutcomeView</from-outcome>
			<to-view-id>/commandable/navigation/outcome.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>commandableNavigationIndexView</from-outcome>
			<to-view-id>/commandable/navigation/index.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>gridIndexView</from-outcome>
			<to-view-id>/grid/index.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>gridCreateView</from-outcome>
			<to-view-id>/grid/create.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>gridEditView</from-outcome>
			<to-view-id>/grid/edit.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>hierarchyOrganigramView</from-outcome>
			<to-view-id>/hierarchy/organigram.jsf</to-view-id>
			<redirect />
		</navigation-case>
				
		<navigation-case>
			<from-outcome>viewIndexView</from-outcome>
			<to-view-id>/view/index.jsf</to-view-id>
			<redirect />
		</navigation-case>
				
		<navigation-case>
			<from-outcome>veryComplexEntityEditView</from-outcome>
			<to-view-id>/crud/verycomplexentity/edit.jsf</to-view-id>
			<redirect />
		</navigation-case>		
	</navigation-rule>
	
</faces-config>
