<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		
		<navigation-case>
			<from-outcome>commandableButtonIndexView</from-outcome>
			<to-view-id>/commandable/button/index.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>gridIndexView</from-outcome>
			<to-view-id>/grid/index.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		
		<navigation-case>
			<from-outcome>myEntityListWindow</from-outcome>
			<to-view-id>/crud/myentity/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>myEntityEditWindow</from-outcome>
			<to-view-id>/crud/myentity/edit.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
	</navigation-rule>
	
</faces-config>
