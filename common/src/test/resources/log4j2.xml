<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n" />
		</Console>
		<Console name="CustomConsole" target="SYSTEM_OUT">
			<Filters>
				
			</Filters>
			<PatternLayout pattern="%d{HH:mm:ss.SSS} %-5level - %marker - %msg%n" />
		</Console>
	</Appenders>
	<Loggers>
		<Logger name="org.cyk.utility.common.LoggingHelperUnitTest"
			level="trace">
			<AppenderRef ref="CustomConsole" />
		</Logger>
		<Logger name="org.cyk.utility.common"
			level="trace">
			<AppenderRef ref="CustomConsole" />
		</Logger>
		<Root level="trace">
			<!--AppenderRef ref="Console"/ -->
		</Root>
	</Loggers>
</Configuration>